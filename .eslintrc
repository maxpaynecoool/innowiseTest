{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
//    "arrow-parens": "off",
    "indent": "off",
    "comma-dangle": "off",
    "linebreak-style": "off",
    "no-unused-vars": "warn"
  },
  "extends": [
    "eslint:recommended",
    "google",
//    "prettier"
  ],
//  "plugins": ["prettier"]
}